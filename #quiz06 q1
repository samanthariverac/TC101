#include <iostream>
using namespace std;

long superpower (long a, long b) {
  long result= 1;
  for (int i=1; i<=b; i++)
  {
    result = result *a;
  }
  return result;
}
int main () {
  long a,b;
  cout <<"Enter the number you want to raise: ";  cin >> a;
  cout <<"Enter the number of the power: "; cin >> b;
  cout << "Here is your number raise to the power:"<< superpower (a, b) << endl;
  return 0;
}
