#include <iostream>
#include <vector>
using namespace std;

 int sumsquares_list (vector <int> ss)
 {
   int i,sums=0;
   for (i=0; i<ss.size(); i++){
     sums= sums + (ss[i]* ss[i]);
   }
   return sums;
 }

 int main () {
vector <int> ss(5);
ss [0]=1;
ss [1]=2;
ss [2]=3;
ss [3]=4;
ss [4]=5;
cout << "Here is the number of the sum of squares:"<< sumsquares_list (ss) << endl;
return 0;
 }
