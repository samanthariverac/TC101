#include <iostream>
using namespace std;

int triangles(int n){
int i,j;
for(i=1; i<=n; i++){
    
    for(j=1; j<=i; j++)
    {
       cout<<"T";
    }
       cout<<"\n";
    } 
	
for(i=n;i>=1;i--){
	
    for(j=1;j<=i;j++)
    {
       cout<<"T";
    }
       cout<< "\n";
    }	
}
int main()
{
int n=5;
triangles(n);
  
    return 0;
}
